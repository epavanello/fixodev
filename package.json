{
  "name": "fixodev-monorepo",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:server": "bun run --cwd apps/server dev",
    "build:server": "bun run --cwd apps/server build",
    "cli": "bun run --cwd apps/server cli",
    "dev:web": "bun run --cwd apps/webapp dev",
    "build:web": "bun run --cwd apps/webapp build"
  }
}
